      "source": [
        "# 🌍 Life Satisfaction in Europe – Wallerstein & Rosa\n",
        "Exploring life satisfaction across European countries using Eurostat-style data.\n",
        "Combines data analysis with sociological theory (World-Systems + ANT)."
      ],
      "metadata": {
        "id": "PrLgx0skwOwG"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# ============================================\n",
        "# Life Satisfaction × Education × Digital Skills (EU 2023)\n",
        "# End-to-end Colab script: data prep → ANCOVA → figures → GitHub push\n",
        "# ============================================\n",
        "\n",
        "# --- 0) Setup & imports\n",
        "!pip -q install pingouin\n",
        "\n",
        "import os, re\n",
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "import statsmodels.api as sm\n",
        "from statsmodels.formula.api import ols\n",
        "from scipy import stats\n",
        "import pingouin as pg\n",
        "from statsmodels.stats.multicomp import pairwise_tukeyhsd\n",
        "\n",
        "os.makedirs('/content/data', exist_ok=True)\n",
        "os.makedirs('/content/figures', exist_ok=True)\n",
        "\n",
        "# --- 1) Load raw files (already uploaded to Colab)\n",
        "desi = pd.read_csv('/content/desi-data.csv')\n",
        "ilc  = pd.read_csv('/content/ilc_pw05_linear.csv')\n",
        "\n",
        "# --- 2) DESI: keep digital skills only; filter year 2023; wide format\n",
        "desi_ds = desi[desi['indicator'].isin(['desi_dsk_bab','desi_dsk_ab'])].copy()\n",
        "desi_ds['indicator'] = desi_ds['indicator'].map({\n",
        "    'desi_dsk_bab': 'basic_digital_skills',\n",
        "    'desi_dsk_ab' : 'above_basic_digital_skills'\n",
        "})\n",
        "desi_ds = desi_ds.rename(columns={'country':'iso2','reference_period':'year'})\n",
        "desi_ds['year'] = pd.to_numeric(desi_ds['year'], errors='coerce')\n",
        "desi_2023 = desi_ds[desi_ds['year'] == 2023].copy()\n",
        "\n",
        "desi_wide = (desi_2023\n",
        "             .pivot_table(index='iso2', columns='indicator', values='value', aggfunc='mean')\n",
        "             .reset_index())\n",
        "\n",
        "print('DESI 2023:', desi_wide.shape)\n",
        "\n",
        "# --- 3) EU-SILC: filter 2023, Total, 16+, valid values\n",
        "ilc['TIME_PERIOD'] = pd.to_numeric(ilc['TIME_PERIOD'], errors='coerce')\n",
        "ilc_2023 = ilc[\n",
        "    (ilc['TIME_PERIOD'] == 2023) &\n",
        "    (ilc['sex'] == 'Total') &\n",
        "    (ilc['age'] == '16 years or over') &\n",
        "    (ilc['OBS_VALUE'] != ':')\n",
        "].copy()\n",
        "ilc_2023['OBS_VALUE'] = pd.to_numeric(ilc_2023['OBS_VALUE'], errors='coerce')\n",
        "\n",
        "ilc_keep = ilc_2023[['geo','TIME_PERIOD','lev_satis','isced11','OBS_VALUE']].rename(columns={\n",
        "    'geo':'country_name',\n",
        "    'TIME_PERIOD':'year',\n",
        "    'lev_satis':'satisfaction_level',\n",
        "    'isced11':'education_level',\n",
        "    'OBS_VALUE':'life_value'\n",
        "})\n",
        "\n",
        "# --- 4) Map country_name → ISO2 (EU27 + neighbors)\n",
        "name_to_iso2 = {\n",
        "    'Austria':'AT','Belgium':'BE','Bulgaria':'BG','Croatia':'HR','Cyprus':'CY','Czechia':'CZ',\n",
        "    'Denmark':'DK','Estonia':'EE','Finland':'FI','France':'FR','Germany':'DE','Greece':'GR',\n",
        "    'Hungary':'HU','Ireland':'IE','Italy':'IT','Latvia':'LV','Lithuania':'LT','Luxembourg':'LU',\n",
        "    'Malta':'MT','Netherlands':'NL','Poland':'PL','Portugal':'PT','Romania':'RO','Slovakia':'SK',\n",
        "    'Slovenia':'SI','Spain':'ES','Sweden':'SE','Iceland':'IS','Norway':'NO','Switzerland':'CH',\n",
        "    'United Kingdom':'UK','Albania':'AL','Serbia':'RS','North Macedonia':'MK','Türkiye':'TR'\n",
        "}\n",
        "ilc_keep['iso2'] = ilc_keep['country_name'].map(name_to_iso2)\n",
        "ilc_keep = ilc_keep.dropna(subset=['iso2']).copy()\n",
        "\n",
        "# --- 5) Weighted life satisfaction index (1–3) by ISO2 × ISCED\n",
        "score_map = {'Low':1, 'Medium':2, 'High':3}\n",
        "ilc_keep['satis_score'] = ilc_keep['satisfaction_level'].map(score_map)\n",
        "ilc_keep['weighted']    = ilc_keep['satis_score'] * ilc_keep['life_value']\n",
        "\n",
        "agg = (ilc_keep\n",
        "       .groupby(['iso2','education_level'], as_index=False)\n",
        "       .agg(life_satisfaction_sum=('weighted','sum'),\n",
        "            weight_sum=('life_value','sum')))\n",
        "\n",
        "ilc_index = agg.copy()\n",
        "ilc_index['life_satisfaction'] = ilc_index['life_satisfaction_sum'] / ilc_index['weight_sum']\n",
        "ilc_index = ilc_index.drop(columns=['life_satisfaction_sum','weight_sum'])\n",
        "\n",
        "# --- 6) Clean education labels → ISCED 0–2 / 3–4 / 5–8 / All levels\n",
        "def simplify_edu(x: str) -> str:\n",
        "    s = str(x).lower().replace('–','-')\n",
        "    if 'all isced' in s: return 'All levels'\n",
        "    if re.search(r'levels?\\s*0-2', s) or 'less than primary' in s or 'lower secondary' in s:\n",
        "        return 'ISCED 0–2'\n",
        "    if re.search(r'levels?\\s*3-4', s) or 'upper secondary' in s or 'post-secondary non-tertiary' in s:\n",
        "        return 'ISCED 3–4'\n",
        "    if re.search(r'levels?\\s*5-8', s) or 'tertiary' in s:\n",
        "        return 'ISCED 5–8'\n",
        "    return x\n",
        "\n",
        "ilc_index['edu_group'] = ilc_index['education_level'].apply(simplify_edu)\n",
        "\n",
        "# --- 7) Merge with DESI (2023)\n",
        "final_2023 = ilc_index.merge(desi_wide, on='iso2', how='inner')\n",
        "final_2023 = (final_2023[['iso2','edu_group','life_satisfaction',\n",
        "                          'above_basic_digital_skills','basic_digital_skills']]\n",
        "              .sort_values(['iso2','edu_group'])\n",
        "              .reset_index(drop=True))\n",
        "\n",
        "# Save merged dataset\n",
        "final_2023.to_csv('/content/data/life_digital_edu_2023.csv', index=False)\n",
        "print('Final dataset:', final_2023.shape)\n",
        "final_2023.head()\n",
        "\n",
        "# --- 8) ANCOVA\n",
        "# Full model with interaction\n",
        "model_full = ols('life_satisfaction ~ C(edu_group) * above_basic_digital_skills', data=final_2023).fit()\n",
        "anova_full = sm.stats.anova_lm(model_full, typ=2)\n",
        "print('\\n=== ANCOVA (full, with interaction) ===')\n",
        "print(anova_full)\n",
        "\n",
        "# Assumptions\n",
        "print('\\nDescriptives:\\n', final_2023[['life_satisfaction','above_basic_digital_skills']].describe())\n",
        "levene = pg.homoscedasticity(final_2023, dv='life_satisfaction', group='edu_group', method='levene')\n",
        "print('\\nLevene test:\\n', levene)\n",
        "shapiro = stats.shapiro(model_full.resid)\n",
        "print('\\nShapiro-Wilk:', shapiro)\n",
        "\n",
        "# If interaction not significant → main-effects model\n",
        "model = ols('life_satisfaction ~ C(edu_group) + above_basic_digital_skills', data=final_2023).fit()\n",
        "anova = sm.stats.anova_lm(model, typ=2)\n",
        "print('\\n=== ANCOVA (main effects) ===')\n",
        "print(anova)\n",
        "print('\\nR^2:', round(model.rsquared, 3))\n",
        "\n",
        "# Partial eta^2\n",
        "ss_error = anova.loc['Residual','sum_sq']\n",
        "effects = anova.index.drop('Residual')\n",
        "petas = {eff: anova.loc[eff,'sum_sq'] / (anova.loc[eff,'sum_sq'] + ss_error) for eff in effects}\n",
        "print('\\nPartial eta^2:', {k: round(v,3) for k,v in petas.items()})\n",
        "\n",
        "# --- 9) Adjusted means (EMMs) at mean digital skills + plot\n",
        "mean_digital = final_2023['above_basic_digital_skills'].mean()\n",
        "levels = final_2023['edu_group'].unique()\n",
        "pred_df = pd.DataFrame({'edu_group': levels, 'above_basic_digital_skills': mean_digital})\n",
        "pred_sum = model.get_prediction(pred_df).summary_frame(alpha=0.05)\n",
        "\n",
        "emm = pred_df.copy()\n",
        "emm['adj_mean'] = pred_sum['mean'].values\n",
        "emm['ci_low']   = pred_sum['mean_ci_lower'].values\n",
        "emm['ci_high']  = pred_sum['mean_ci_upper'].values\n",
        "order = [x for x in ['ISCED 0–2','ISCED 3–4','ISCED 5–8','All levels'] if x in emm['edu_group'].tolist()]\n",
        "emm = emm.set_index('edu_group').loc[order].reset_index()\n",
        "display(emm)\n",
        "\n",
        "plt.figure(figsize=(6,4))\n",
        "x = np.arange(len(emm))\n",
        "y = emm['adj_mean'].values\n",
        "yerr = [y - emm['ci_low'].values, emm['ci_high'].values - y]\n",
        "plt.errorbar(x, y, yerr=yerr, fmt='o', capsize=5)\n",
        "plt.xticks(x, emm['edu_group'])\n",
        "plt.ylabel('Adjusted life satisfaction (1–3)')\n",
        "plt.title('Adjusted means by education (controlled for digital skills)')\n",
        "plt.tight_layout()\n",
        "plt.savefig('/content/figures/adjusted_means_edu.png', dpi=300)\n",
        "plt.show()\n",
        "\n",
        "# --- 10) Scatter + within-group OLS lines (visualizing (no) interaction)\n",
        "plt.figure(figsize=(7,5))\n",
        "for g in final_2023['edu_group'].unique():\n",
        "    sub = final_2023[final_2023['edu_group']==g]\n",
        "    plt.scatter(sub['above_basic_digital_skills'], sub['life_satisfaction'], alpha=0.75, label=g)\n",
        "    if len(sub) >= 2:\n",
        "        Xg = sm.add_constant(sub['above_basic_digital_skills'])\n",
        "        yg = sub['life_satisfaction']\n",
        "        fitg = sm.OLS(yg, Xg).fit()\n",
        "        xs = np.linspace(sub['above_basic_digital_skills'].min(), sub['above_basic_digital_skills'].max(), 25)\n",
        "        ys = fitg.params[0] + fitg.params[1]*xs\n",
        "        plt.plot(xs, ys)\n",
        "plt.xlabel('Above-basic digital skills (%)')\n",
        "plt.ylabel('Life satisfaction (1–3)')\n",
        "plt.title('Effect of digital skills by education group (lines = within-group OLS)')\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.savefig('/content/figures/digital_skills_by_edu.png', dpi=300)\n",
        "plt.show()\n",
        "\n",
        "# --- 11) Tukey HSD on residualized DV (optional post-hoc)\n",
        "X = sm.add_constant(final_2023['above_basic_digital_skills'])\n",
        "y = final_2023['life_satisfaction']\n",
        "resid_model = sm.OLS(y, X).fit()\n",
        "final_2023['life_resid'] = resid_model.resid\n",
        "\n",
        "tuk = pairwise_tukeyhsd(endog=final_2023['life_resid'],\n",
        "                        groups=final_2023['edu_group'],\n",
        "                        alpha=0.05)\n",
        "print('\\n=== Tukey HSD on residualized DV ===')\n",
        "print(tuk.summary())\n",
        "\n",
          ]
        }
      ]
    }
  ]
